{
    "name": "kinesio/site",
    "type": "project",
    "description": "Landing",
    "keywords": [
        "framework",
        "laravel",
        "site"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "arcanedev/log-viewer": "8.x",
        "davmixcool/laravel-meta-manager": "^1.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "jamesmills/laravel-timezone": "^1.9",
        "langleyfoxall/laravel-nist-password-rules": "^4.1",
        "laravel/framework": "^8.40",
        "laravel/passport": "^10.1",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.5",
        "orchid/platform": "^10.11",
        "rappasoft/lockout": "^3.0",
        "riverskies/laravel-mobile-detect": "^1.3",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/laravel-permission": "^5.5",
        "spatie/schema-org": "^3.8",
        "tabuna/breadcrumbs": "^2.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "brianium/paratest": "^6.2",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [],
        "clear-all": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "composer dumpautoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache"
        ],
        "reset": [
            "composer clear-all",
            "composer cache-all"
        ],
        "test": "@php artisan test --parallel",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Passport\\PassportServiceProvider",
                "Riverskies\\Laravel\\MobileDetect\\MobileDetectServiceProvider",
                "Davmixcool\\MetaManager\\MetaServiceProvider",
                "Intervention\\Image\\ImageServiceProvider",
                "Spatie\\Permission\\PermissionServiceProvider"
            ],
            "aliases": {
                "MobileDetect": "Riverskies\\Laravel\\MobileDetect\\Facades\\MobileDetect",
                "Image": "Intervention\\Image\\Facades\\Image",
                "Debugbar": "Barryvdh\\Debugbar\\Facades\\Debugbar"
            },
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "treeware/plant": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
